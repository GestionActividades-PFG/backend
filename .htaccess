<IfModule mod_rewrite.c>
	RewriteEngine On
	RewriteBase /
	RewriteCond %{REQUEST_FILENAME} !-f
	RewriteCond %{REQUEST_FILENAME} !-d
	RewriteRule .* Proyectos/TFG-EVG/evg/index.php/$0 [PT,L]

	#REVISAR
	RewriteRule ^index\.php/?$ - [L,R=301,NC]

</IfModule>

<IfModule !mod_rewrite.c>
	ErrorDocument 404 /index.php
</IfModule>